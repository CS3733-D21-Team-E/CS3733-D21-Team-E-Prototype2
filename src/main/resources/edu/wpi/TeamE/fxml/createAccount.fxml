<VBox prefHeight="800.0" prefWidth="1200.0" stylesheets="@mainStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.TeamE.views.createAccount">
      <children>
      <AnchorPane id="app-bar-top" prefWidth="1012.0">
         <children>
            <VBox id="app-bar-top" layoutX="-10.0" layoutY="-10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox id="app-bar-top-upper" maxHeight="25.0" minHeight="25.0" prefHeight="25.0" stylesheets="@mainStyle.css">
                     <children>
                        <FlowPane id="app-bar-top-upper" alignment="CENTER_RIGHT" columnHalignment="RIGHT" HBox.hgrow="ALWAYS">
                           <children>
                              <Rectangle fx:id="fullscreen" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="13.0" stroke="#00000004" strokeType="INSIDE" style="-fx-fill: -fx--primary;" width="13.0" />
                              <Circle fx:id="hide" fill="#e10a0a" radius="7.0" stroke="#00000004" strokeType="INSIDE" style="-fx-fill: -fx--primary;">
                                 <FlowPane.margin>
                                    <Insets bottom="5.0" left="10.0" top="5.0" />
                                 </FlowPane.margin>
                              </Circle>
                              <Polygon fx:id="exit" fill="DODGERBLUE" rotate="180.0" stroke="#00000001" strokeType="INSIDE" style="-fx-fill: -fx--primary;">
                                 <points>
                                    <Double fx:value="-7.0" />
                                    <Double fx:value="7.0" />
                                    <Double fx:value="7.0" />
                                    <Double fx:value="7.0" />
                                    <Double fx:value="0.0" />
                                    <Double fx:value="-7.0" />
                                 </points>
                                 <FlowPane.margin>
                                    <Insets left="10.0" />
                                 </FlowPane.margin>
                              </Polygon>
                           </children>
                           <padding>
                              <Insets right="10.0" />
                           </padding>
                        </FlowPane>
                     </children>
                  </HBox>
                  <HBox id="app-bar-top-main" maxHeight="40.0" minHeight="40.0" prefHeight="40.0">
                     <children>
                        <FlowPane style="-fx-background-color: -fx--primary;" HBox.hgrow="ALWAYS">
                           <children>
                              <Label prefHeight="38.0" prefWidth="486.0" text="Create Account" textFill="WHITE">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                                 <FlowPane.margin>
                                    <Insets left="10.0" />
                                 </FlowPane.margin>
                              </Label>
                           </children>
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </FlowPane>
                        <FlowPane alignment="CENTER_RIGHT" columnHalignment="RIGHT" style="-fx-background-color: -fx--primary;" HBox.hgrow="ALWAYS">
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </FlowPane>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane VBox.vgrow="ALWAYS">
         <children>
            <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <bottom>
                  <AnchorPane prefHeight="45.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
                     <children>
                        <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <JFXButton fx:id="back" onAction="#toLogin" prefHeight="29.0" prefWidth="60.0" style="-fx-background-color: -fx--primary-dark; -fx-border-color: -fx--primary-light;" text="Back" textFill="WHITE">
                                 <HBox.margin>
                                    <Insets left="20.0" />
                                 </HBox.margin>
                              </JFXButton>
                           </children>
                        </HBox>
                     </children>
                  </AnchorPane>
               </bottom>
               <right>
                          <VBox prefWidth="0.0" BorderPane.alignment="CENTER" />
               </right>
               <center>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                     <children>
                                  <Label alignment="CENTER" prefHeight="23.0" prefWidth="381.0" text="Please Enter Credentials">
                                      <font>
                                          <Font size="18.0" />
                                      </font>
                                      <VBox.margin>
                                          <Insets />
                                      </VBox.margin>
                                  </Label>
                                    <JFXTextField fx:id="email" maxWidth="300.0" prefColumnCount="0" prefWidth="300.0" promptText="Email">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                                    </JFXTextField>
                                    <JFXTextField fx:id="password" maxWidth="300.0" prefHeight="27.0" prefWidth="300.0" promptText="Password">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                                    </JFXTextField>
                        <JFXTextField fx:id="firstName" maxWidth="300.0" prefWidth="300.0" promptText="First Name">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </JFXTextField>
                        <JFXTextField fx:id="lastName" maxWidth="300.0" prefWidth="300.0" promptText="Last Name">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </JFXTextField>
                                    <Button fx:id="submitAccountButton" mnemonicParsing="false" onAction="#createAccountButton" prefHeight="29.0" prefWidth="66.0" style="-fx-background-color: -fx--primary-dark; -fx-border-color: -fx--primary-light;" text="Submit" textFill="WHITE">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </center>
            </BorderPane>
         </children>
      </AnchorPane>
      </children>
  </VBox>
