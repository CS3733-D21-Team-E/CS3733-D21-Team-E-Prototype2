<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="800.0" prefWidth="1200.0" stylesheets="@mainStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.TeamE.views.UserManagement">
   <children>
      <AnchorPane id="app-bar-top" fx:id="appBarAnchorPane" prefHeight="65.0" prefWidth="1012.0" />
      <AnchorPane prefHeight="685.0" prefWidth="1200.0">
         <children>
            <VBox prefHeight="318.0" prefWidth="591.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <JFXTreeTableView fx:id="treeTableView" prefHeight="512.0" prefWidth="1160.0" />
                  <HBox prefHeight="477.0" prefWidth="1160.0">
                     <children>
                        <VBox prefHeight="476.0" prefWidth="1443.0">
                           <children>
                              <FlowPane prefHeight="41.0" prefWidth="297.0">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#addUserButton" text="Add User">
                                       <FlowPane.margin>
                                          <Insets right="10.0" top="15.0" />
                                       </FlowPane.margin>
                                    </Button>
                                    <Button mnemonicParsing="false" onAction="#editUserButton" text="Edit User">
                                       <FlowPane.margin>
                                          <Insets right="10.0" top="15.0" />
                                       </FlowPane.margin>
                                    </Button>
                                    <Button mnemonicParsing="false" onAction="#deleteUserButton" text="Delete User">
                                       <FlowPane.margin>
                                          <Insets top="15.0" />
                                       </FlowPane.margin>
                                    </Button>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </FlowPane>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="58.0" prefWidth="1200.0">
         <children>
            <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <JFXButton fx:id="backButton" onAction="#toNavigation" prefHeight="29.0" prefWidth="60.0" style="-fx-background-color: -fx--primary-dark; -fx-border-color: -fx--primary-light;" styleClass="submit-button" text="Back">
                     <GridPane.margin>
                        <Insets bottom="20.0" left="20.0" />
                     </GridPane.margin>
                  </JFXButton>
                  <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#startTableButton" text="Refresh" GridPane.columnIndex="1" GridPane.halignment="RIGHT">
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                     <GridPane.margin>
                        <Insets bottom="20.0" right="20.0" />
                     </GridPane.margin>
                  </Button>
               </children>
            </GridPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
