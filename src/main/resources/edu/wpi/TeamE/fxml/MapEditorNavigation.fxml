                        <Button fx:id="toNodeEditorButton" mnemonicParsing="false" onAction="#toNodeMapEditor" prefHeight="22.0" prefWidth="300.0" style="-fx-background-color: -fx--primary-dark; -fx-border-color: -fx--primary-light;" text="Node Map Editor" textFill="WHITE">
